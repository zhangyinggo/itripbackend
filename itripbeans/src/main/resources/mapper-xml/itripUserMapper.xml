<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.kgc.itrip.mapper.ItripUserMapper">

    <select id="getById" resultType="ItripUser">
        select
                id as id,         <!--主键-->
                userCode as userCode,         <!--若是第三方登录，系统将自动生成唯一账号；自注册用户则为邮箱或者手机号-->
                userPassword as userPassword,         <!--若是第三方登录，系统将自动生成唯一密码；自注册用户则为自定义密码-->
                userType as userType,         <!--用户类型（标识：0 自注册用户 1 微信登录 2 QQ登录 3 微博登录）-->
                flatID as flatID,         <!--平台ID（根据不同登录用户，进行相应存入：自注册用户主键ID、微信ID、QQID、微博ID）-->
                userName as userName,         <!--用户昵称-->
                weChat as weChat,         <!--微信号-->
                QQ as QQ,         <!--qq账号-->
                weibo as weibo,         <!--微博账号-->
                baidu as baidu,         <!--百度账号-->
                creationDate as creationDate,         <!---->
                createdBy as createdBy,         <!---->
                modifyDate as modifyDate,         <!---->
                modifiedBy as modifiedBy,         <!---->
                activated as activated          <!--是否激活,(0 false，1 true,默认是0)-->
        from itrip_user
        <trim prefix="where" prefixOverrides="and | or">
            <if test="id != null">
                and id=#{id}
            </if>
        </trim>
    </select>

    <select id="getListByMap" resultType="ItripUser"
            parameterType="java.util.Map">
            select
                id as id,      <!--主键-->
                userCode as userCode,      <!--若是第三方登录，系统将自动生成唯一账号；自注册用户则为邮箱或者手机号-->
                userPassword as userPassword,      <!--若是第三方登录，系统将自动生成唯一密码；自注册用户则为自定义密码-->
                userType as userType,      <!--用户类型（标识：0 自注册用户 1 微信登录 2 QQ登录 3 微博登录）-->
                flatID as flatID,      <!--平台ID（根据不同登录用户，进行相应存入：自注册用户主键ID、微信ID、QQID、微博ID）-->
                userName as userName,      <!--用户昵称-->
                weChat as weChat,      <!--微信号-->
                QQ as QQ,      <!--qq账号-->
                weibo as weibo,      <!--微博账号-->
                baidu as baidu,      <!--百度账号-->
                creationDate as creationDate,      <!---->
                createdBy as createdBy,      <!---->
                modifyDate as modifyDate,      <!---->
                modifiedBy as modifiedBy,      <!---->
                activated as activated       <!--是否激活,(0 false，1 true,默认是0)-->
            from itrip_user
        <trim prefix="where" prefixOverrides="and | or">
                <if test="id != null and id!=''">
                    and id=#{id}
                </if>
                <if test="userCode != null and userCode!=''">
                    and userCode=#{userCode}
                </if>
                <if test="userPassword != null and userPassword!=''">
                    and userPassword=#{userPassword}
                </if>
                <if test="userType != null and (userType!='' or userType==0)">
                    and userType=#{userType}
                </if>
                <if test="flatID != null and flatID!=''">
                    and flatID=#{flatID}
                </if>
                <if test="userName != null and userName!=''">
                    and userName=#{userName}
                </if>
                <if test="weChat != null and weChat!=''">
                    and weChat=#{weChat}
                </if>
                <if test="QQ != null and QQ!=''">
                    and QQ=#{QQ}
                </if>
                <if test="weibo != null and weibo!=''">
                    and weibo=#{weibo}
                </if>
                <if test="baidu != null and baidu!=''">
                    and baidu=#{baidu}
                </if>
                <if test="creationDate != null and creationDate!=''">
                    and creationDate=#{creationDate}
                </if>
                <if test="createdBy != null and createdBy!=''">
                    and createdBy=#{createdBy}
                </if>
                <if test="modifyDate != null and modifyDate!=''">
                    and modifyDate=#{modifyDate}
                </if>
                <if test="modifiedBy != null and modifiedBy!=''">
                    and modifiedBy=#{modifiedBy}
                </if>
                <if test="activated != null and (activated!='' or activated==0)">
                    and activated=#{activated}
                </if>
        </trim>
        order by creationDate desc
        <if test="beginPos != null and beginPos!='' and pageSize != null  and pageSize !='' ">
            limit #{beginPos},#{pageSize}
        </if>
    </select>

    <select id="getCountByMap" resultType="Integer" parameterType="java.util.Map">
        select count(1) from itrip_user
        <trim prefix="where" prefixOverrides="and | or">
                    <if test="id != null and id!=''">
                        and id=#{id}
                    </if>
                    <if test="userCode != null and userCode!=''">
                        and userCode=#{userCode}
                    </if>
                    <if test="userPassword != null and userPassword!=''">
                        and userPassword=#{userPassword}
                    </if>
                    <if test="userType != null and (userType!='' or userType==0)">
                        and userType=#{userType}
                    </if>
                    <if test="flatID != null and flatID!=''">
                        and flatID=#{flatID}
                    </if>
                    <if test="userName != null and userName!=''">
                        and userName=#{userName}
                    </if>
                    <if test="weChat != null and weChat!=''">
                        and weChat=#{weChat}
                    </if>
                    <if test="QQ != null and QQ!=''">
                        and QQ=#{QQ}
                    </if>
                    <if test="weibo != null and weibo!=''">
                        and weibo=#{weibo}
                    </if>
                    <if test="baidu != null and baidu!=''">
                        and baidu=#{baidu}
                    </if>
                    <if test="creationDate != null and creationDate!=''">
                        and creationDate=#{creationDate}
                    </if>
                    <if test="createdBy != null and createdBy!=''">
                        and createdBy=#{createdBy}
                    </if>
                    <if test="modifyDate != null and modifyDate!=''">
                        and modifyDate=#{modifyDate}
                    </if>
                    <if test="modifiedBy != null and modifiedBy!=''">
                        and modifiedBy=#{modifiedBy}
                    </if>
                    <if test="activated != null and (activated!='' or activated==0)">
                        and activated=#{activated}
                    </if>
        </trim>
    </select>

    <insert id="save" parameterType="ItripUser">
        insert into itrip_user(
            userCode ,      <!--若是第三方登录，系统将自动生成唯一账号；自注册用户则为邮箱或者手机号-->
            userPassword ,      <!--若是第三方登录，系统将自动生成唯一密码；自注册用户则为自定义密码-->
            userType ,      <!--用户类型（标识：0 自注册用户 1 微信登录 2 QQ登录 3 微博登录）-->
            flatID ,      <!--平台ID（根据不同登录用户，进行相应存入：自注册用户主键ID、微信ID、QQID、微博ID）-->
            userName ,      <!--用户昵称-->
            weChat ,      <!--微信号-->
            QQ ,      <!--qq账号-->
            weibo ,      <!--微博账号-->
            baidu ,      <!--百度账号-->
            creationDate ,      <!---->
            createdBy ,      <!---->
            modifyDate ,      <!---->
            modifiedBy ,      <!---->
            activated       <!--是否激活,(0 false，1 true,默认是0)-->
        )
        values(
                    #{userCode} ,      <!--若是第三方登录，系统将自动生成唯一账号；自注册用户则为邮箱或者手机号-->
                    #{userPassword} ,      <!--若是第三方登录，系统将自动生成唯一密码；自注册用户则为自定义密码-->
                    #{userType} ,      <!--用户类型（标识：0 自注册用户 1 微信登录 2 QQ登录 3 微博登录）-->
                    #{flatID} ,      <!--平台ID（根据不同登录用户，进行相应存入：自注册用户主键ID、微信ID、QQID、微博ID）-->
                    #{userName} ,      <!--用户昵称-->
                    #{weChat} ,      <!--微信号-->
                    #{QQ} ,      <!--qq账号-->
                    #{weibo} ,      <!--微博账号-->
                    #{baidu} ,      <!--百度账号-->
                    #{creationDate} ,      <!---->
                    #{createdBy} ,      <!---->
                    #{modifyDate} ,      <!---->
                    #{modifiedBy} ,      <!---->
                    #{activated}       <!--是否激活,(0 false，1 true,默认是0)-->
        )
    </insert>

    <update id="modify" parameterType="ItripUser">
        update itrip_user
        <trim prefix="set" suffixOverrides="," suffix="where id= #{id}">
                <if test="id != null and id!=''">
                     id=#{id},
                </if>
                <if test="userCode != null and userCode!=''">
                     userCode=#{userCode},
                </if>
                <if test="userPassword != null and userPassword!=''">
                     userPassword=#{userPassword},
                </if>
                <if test="userType != null and (userType!='' or userType==0)">
                     userType=#{userType},
                </if>
                <if test="flatID != null and flatID!=''">
                     flatID=#{flatID},
                </if>
                <if test="userName != null and userName!=''">
                     userName=#{userName},
                </if>
                <if test="weChat != null and weChat!=''">
                     weChat=#{weChat},
                </if>
                <if test="QQ != null and QQ!=''">
                     QQ=#{QQ},
                </if>
                <if test="weibo != null and weibo!=''">
                     weibo=#{weibo},
                </if>
                <if test="baidu != null and baidu!=''">
                     baidu=#{baidu},
                </if>
                <if test="creationDate != null and creationDate!=''">
                     creationDate=#{creationDate},
                </if>
                <if test="createdBy != null and createdBy!=''">
                     createdBy=#{createdBy},
                </if>
                <if test="modifyDate != null and modifyDate!=''">
                     modifyDate=#{modifyDate},
                </if>
                <if test="modifiedBy != null and modifiedBy!=''">
                     modifiedBy=#{modifiedBy},
                </if>
                <if test="activated != null and (activated!='' or activated==0)">
                     activated=#{activated},
                </if>
        </trim>
    </update>

    <delete id="removeById" parameterType="Long">
        delete from itrip_user where id = #{id}
    </delete>
</mapper>